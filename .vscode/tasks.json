			"problemMatcher": [
				"$msCompile"
			],
			"group": "test"
		},
		{
			"label": "Run ClipImageEmbeddingServiceTest",
			"type": "shell",
			"command": "dotnet test .\\TestMarketAssistant\\TestMarketAssistant.csproj -c Debug --filter \"FullyQualifiedName~ClipImageEmbeddingServiceTest\" --no-build --nologo",
			"isBackground": false,
